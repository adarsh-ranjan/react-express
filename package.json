{
  "name": "web-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "client-install": "cd client && npm install",
    "client": "npm start --prefix client",
    "server": "nodemon bin/www",
    "dev": "concurrently \"nodemon start\" \"npm run client\"",
    "build": "npm run build --prefix client"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cores": "^0.8.5",
    "cors": "^2.8.5",
    "nodemon": "^1.18.10"
  }
}
